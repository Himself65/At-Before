<Application
    x:Class="At_Before.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:At_Before"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Light" Source="/Style/LightDictionary.xaml"/>
                <ResourceDictionary x:Key="Dark" Source="/Style/DarkDictionary.xaml"/>
                <ResourceDictionary x:Key="Main" Source="/Style/MainDictionary.xaml"/>
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
